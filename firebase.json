{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "jetpalz",
          "appId": "1:416263953272:android:1ea47e73ee31a35516c68d",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "jetpalz",
          "appId": "1:416263953272:ios:efde9f791ee457d916c68d",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "jetpalz",
          "configurations": {
            "android": "1:416263953272:android:1ea47e73ee31a35516c68d",
            "ios": "1:416263953272:ios:efde9f791ee457d916c68d"
          }
        }
      }
    }
  },
  "emulators": {
    "singleProjectMode": true,
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 8080
    },
    "storage": {
      "port": 9199,
      "rules": "firebase-storage-rules.json"
    },
    "ui": {
      "enabled": true
    }
  },
  "storage": {
    "rules": "storage.rules"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint"
      ]
    }
  ]
}
